{
    "name": "rawphp/raw-error-handler",
    "type": "library",
    "description": "RawErrorHandler is a simple error handler used by RawPHP framework and other PHP applications.",
    "keywords": [
        "error",
        "exception",
        "handler"
    ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawErrorHandler\\": "src/RawPHP/RawErrorHandler/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
